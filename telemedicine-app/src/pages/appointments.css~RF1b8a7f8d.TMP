
.Appointments-container-outer {
   
    height: 100vh;
    width: 100vw;
    
}

.Appointments-container-inner {
    text-align: -webkit-center;
    padding: 5%;
    padding-top: 10%;
    height: 100vh;
    width: 100vw;
    border-radius: 30px;
}

.Appointments-card {
    padding: 10%;
    box-shadow: 0 5px 15px 5px rgb(0 0 0 / 20%);
    place-content: center;
    width: 100%;
    height: 100%;
    border-radius: 30px;
    overflow-y: auto;
}

.button {
    background-color: limegreen;
    border: none;
    border-radius: 25px;
    color: white;
    padding: 6px;
    text-align: center;
    text-decoration: none;
    font-size: 20px;
    width: 200px;
    height: 40px;
    float: right;
    margin-top: -100px;
    margin-right: -50px;
}







.app {
    font-family: sans-serif;
    display: flex;
    align-items: center;
    justify-content: center;
    height: 100vh;
    flex-direction: column;
    gap: 20px;
}

.sign {
    font-size: 25px;
}

.accordian {
    width: 1050px;
    border: 1px solid rgb(133, 133, 133);
}

.accordian-header {
    font-weight: 600;
    font-size: 18px;
    cursor: pointer;
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 15px 15px;
    border: 2px solid rgb(133, 133, 133);
    background-color: rgb(255, 255, 143);
}

.accordian-body {
    padding: 15px 5px;
}